tuple = ("Ekiti", 750, 'Oshogbo', 250, "Akure", 500)
s_tuple = ("Abeokuta", 300, "Ogbomoso")

# Prints the complete tuple
print (tuple)

# Prints last element of the tuple
print (tuple[-1])

# Prints elements of the tuple starting from 2n dtill 3rd
print (tuple[2:4])

# Prints elements of te tuple starting from 3rd elements
print (tuple[3:])

# Prints the contents of the tuple twice
print (s_tuple * 3)

# Prints concatenated tuples
print (tuple + s_tuple)